@{
Layout = null;
}
<!DOCTYPE html>
<html>

<head>
	<title>Personalities Test Widget Flat Responsive Widget Template :: w3layouts</title>
	<!-- for-mobile-apps -->
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<!-- //for-mobile-apps -->

	<!-- font-awesome icons -->
	<link rel="stylesheet" href="css/font-awesome.min.css" />
	<!-- //font-awesome icons -->
	<link href="~/Content/css/track-style.css" rel="stylesheet" type="text/css" media="all" />
	<link
		href="//fonts.googleapis.com/css?family=Ubuntu:300,300i,400,400i,500,500i,700,700i&subset=cyrillic,cyrillic-ext,greek,greek-ext,latin-ext"
		rel="stylesheet">
</head>

<body>
	<div class="main">
		<h1>TRACK YOUR ACCOUNT</h1>
		<div class="w3layouts_main_grids">
			<div class="w3layouts_main_grid">
				<div class="w3-agile-info"></div>
				<div class="w3layouts_main_grid1_w3">
					<div class="w3-user">
						<i class="fa fa-search" aria-hidden="true"></i>
					</div>
					<form action="#" method="post">
						<input type="text" name="Username" placeholder="Account id" required="">
						<input type="email" name="Email" placeholder="Phone number" required="">
						<input type="submit" class="btn" value="Track">
					</form>
					<h6><a href="#">Forgot Account Id?</a></h6>
				</div>
			</div>
			<div class="w3_agile_main_grid">
				<div class="w3-agile-info"></div>
				<div class="sap_tabs">
					<div id="horizontalTab" style="display: block; width: 100%; margin: 0px;">
						<ul class="resp-tabs-list">
							<li style='display:none' class="resp-tab-item" aria-controls="tab_item-0" role="tab">
								<span></span>

						</ul>
						<div class="resp-tabs-container">
							<div class="tab-1 resp-tab-content" aria-labelledby="tab_item-0">
								<div class="agile-tab-top">
									<div class="w3-user">
										<i class="fa fa-user" aria-hidden="true"></i>
									</div>
									<h4>ABC XYZ</h4>
									<div class="left-w3ls">
										<ul class="address">
											<li>
												<ul class="address-text">
													<li><b>PHONE </b></li>
													<li>0912345678</li>
												</ul>
											</li>
											<li>
												<ul class="address-text">
													<li><b>ADDRESS </b></li>
													<li>Nguyễn Văn Cừ</li>
												</ul>
											</li>
											<li>
												<ul class="address-text">
													<li><b>E-MAIL </b></li>
													<li><a href="#">mail@mail.com</a></li>
												</ul>
											</li>
											<li>
												<ul class="address-text wide">
													<li><b>Order Status </b></li>
													<li><a style='color:red' href="#">PENDING</a></li>
												</ul>
											</li>
										</ul>
									</div>
								</div>
							</div>
						</div>
					</div>
					<div class="clear"> </div>
				</div>

			</div>
			<div class="agile_quick_search">
				<div class="w3-agile-info"></div>
				<div class="wthree-info">
					<div class="w3-user">
						<i class="fa fa-info" aria-hidden="true"></i>
					</div>
					<h4>Service info</h4>
					<div class="left-w3ls">
						<ul class="address">
							<li>
								<ul class="address-text">
									<li><b>Type </b></li>
									<li>Dial-up</li>
								</ul>
							</li>
							<li>
								<ul class="address-text wide ">
									<li><b>Rent Period </b></li>
									<li>Monthly</li>
								</ul>
							</li>
							<li>
								<ul class="address-text wide">
									<li><b>Due Payment </b></li>
									<li><a class='money' href="#">0 USD</a></li>
								</ul>
							</li>
							<li>
								<ul class="address-text wide">
									<li><b>Check bills </b></li>
									<li><a style='color:red' href="#">No bill</a></li>
								</ul>
							</li>
						</ul>
					</div>
				</div>

			</div>
			<div class="clear"> </div>
		</div>


	</div>
	<!-- js -->
	<script type="text/javascript" src="~/Content/js/jquery-2.1.4.min.js"></script>
	<script src="~/Content/js/easyResponsiveTabs.js" type="text/javascript"></script>
	<script type="text/javascript">
		$(document).ready(function () {
			$('#horizontalTab').easyResponsiveTabs({
				type: 'default', //Types: default, vertical, accordion           
				width: 'auto', //auto or any width like 600px
				fit: true   // 100% fit in a container
			});
		});
	</script>
	<!-- //js -->
</body>

</html>